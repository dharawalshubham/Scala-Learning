
// SCALA MATCH EXPRESSIONS

object Match {
  def main(args: Array[String]){
    val age = 3
    age match{
      case 18 => println("Matched")
      case 19 => println("Not Matched")
      case 20 => println("Not Matched")
      case _ =>  println("Variable Value Doesn't match any case")


    }
  }
}