
// LISTS in scala

object LISTS {
  val mylist = List(1,2,3,4,5)
  val name   = List("Shubham","Sarthak","Vikas","Archit")
  def main(args: Array[String]){
    println(mylist)
    println(name)
    val mylist2 = 0 +: mylist
    val mylist3 = mylist2 :+ 6
    println(mylist2)
    println(mylist3)
  }
}